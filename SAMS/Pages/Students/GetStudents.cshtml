@page
@model SAMS.Pages.Students.GetStudentsModel
@{
}

<table class="table">
    <thead>
        <tr>
            <th>  Student NO   </th>
            <th>    Name  </th>
            <th>  Address  </th>
            <th>    Has Room  </th>
            <th>  Registration Date   </th>
            <th>  Details   </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Students)
        {
        <tr>
            <td> @item.Student_No </td>
            <td> @item.SName </td>
            <td> @item.SAddress </td>
            <td> @item.Has_Room </td>
            <td> @item.RegistrationDate </td>
            <td> <p><a asp-page="/Students/GetStudentInfo" asp-route-id="@item.Student_No">Details</a></p></td>
        </tr>
        }
    </tbody>
</table>

